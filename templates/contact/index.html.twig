{% extends 'base.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="flash-{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    <div class="container">
        <h1>contact</h1>
        {% include 'components/_form.html.twig' with {'label_button':'valider'} %}
    </div>
{% endblock %}
